package com.github.CreatureOX.eemod.item;

import com.github.CreatureOX.eemod.Main;

import net.minecraft.item.ItemSword;

public class ExcaliburSword extends ItemSword{

	public ExcaliburSword(ToolMaterial material) {
		super(material);
		// TODO 自动生成的构造函数存根
	}
	public ExcaliburSword(String unlocalizedName, ToolMaterial material) {
        super(material);
        this.setUnlocalizedName(unlocalizedName);
        this.setTextureName(Main.MODID + ":" + unlocalizedName);
}

}
