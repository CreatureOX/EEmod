package com.github.CreatureOX.eemod.client;

import net.minecraft.client.settings.KeyBinding;
import cpw.mods.fml.client.registry.ClientRegistry;
import org.lwjgl.input.Keyboard;

public class ModKeyBinding {

	public static KeyBinding showTime;
	
	public ModKeyBinding(){
		ModKeyBinding.showTime = new KeyBinding("key.eemod.showTime",Keyboard.KEY_H,"key.categories.eemod");
		ClientRegistry.registerKeyBinding(ModKeyBinding.showTime);
	}
}
