package com.github.CreatureOX.eemod.item;

import com.github.CreatureOX.eemod.Main;

import cpw.mods.fml.common.registry.GameRegistry;
import net.minecraft.item.Item;

public class ModItems {

	public static Item experimentItem;
	
	public static final void init(){
		
		experimentItem = new Item().setUnlocalizedName("experimentItem").setTextureName(Main.MODID+":experimentItem");
		GameRegistry.registerItem(experimentItem, "experimentItem");
	}
}
